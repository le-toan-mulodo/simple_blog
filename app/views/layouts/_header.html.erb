<header class="navbar navbar-fixed-top">
	<div class="navbar-inner">
		<div class="container">
			<%= link_to "Interactive Blog", 'http://google.com',  class: 'brand'%>
			<%= cookies[:teo]%>
			<nav>
				<ul class="nav pull-right">
					<li>
						<%= link_to "Home", root_path %>
					</li>
					<li>
						<%= link_to "Blogs", posts_path %>
					</li>
					<!-- <%= debug(session[:remember_token])%> -->
					<% if signed_in? %>
					<li>
						<%= link_to "Users", users_path %>
					</li>
					<li>
						<%= link_to "Your articles", current_user %>
					</li>
					<li id="fat-menu" class="dropdown">
						<a href="#" id="drop-toggle" class="dropdown-toggle" data-toggle="dropdown"> <%= "#{current_user.name} (#{current_user.role.name})" %> <b class="caret"></b> </a>
						<ul class="dropdown-menu">
							<li>
								<%= link_to "Settings", edit_user_path(current_user) %>
							</li>
							<li class="divider"></li>
							<li>
								<%= link_to "Sign out", signout_path, method: "delete" %>
							</li>
						</ul>
					</li>
					<% else %>
					<li>
						<%= link_to "Sign in", signin_path %>
					</li>
					<% end %>
				</ul>
			</nav>
		</div>

	</div>
</header>